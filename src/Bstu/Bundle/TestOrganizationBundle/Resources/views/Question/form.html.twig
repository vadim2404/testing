{{ form_start(form) }}
{{ form_errors(form) }}
{{ form_row(form.question) }}
{% if form.type is defined %}
    {{ form_row(form.type) }}
{% endif %}
{% if form.variants is defined %}
    <ul id="variants-field-list" data-prototype="{{ form_widget(form.variants.vars.prototype) | e }}">
    {% for variant in form.variants %}
        <li>
            {{ form_errors(variant) }}
            {{ form_widget(variant) }}
        </li>
    {% endfor %}
    </ul>
    {{ form_row(form.variants) }}
    <a href="#" id="add-another-variant">Добавить новый вариант</a>
{% endif %}
{{ form_end(form) }}